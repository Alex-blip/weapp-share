.my {
  max-height: 100%;
  width: 100%;
  height: 100%;
}

.head {
  width: 100%;
  height: 260rpx;
  background-color: #1c92d2;
  position: relative;
}

.card {
  width: 85%;
  height: 220rpx;
  background-color: #ffff;
  margin: 0 auto;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  padding: 10rpx 30rpx 10rpx 30rpx;
  display: flex;
  align-items: center;
  position: absolute;
  bottom: 0;
  left: 3.5%;
}
.info{
  margin-left: 50rpx;
}
.nickname {
  font-size: 40rpx;
  font-weight: 700;
}
.item {
  font-size: 22rpx;
  padding: 5rpx 30rpx;
  border-radius: 10rpx;
  color: #eeee;
}
.star{
  background-color: orange;
}
.count{
  margin-left: 20px;
  background-color: red;
}
.title{
  font-size: 35rpx;
  padding: 10px 7.5%;
}
