.input-container{
  width: 100%;
  height: 50px;
  background-color: #1c92d2;
  display: flex;
  align-items: center;
}
.input-container text{
  margin-left: 20rpx;
  color: white;
  font-weight: 700;
}
.input-wrap{
  height: 40px;
  flex: 1;
  background-color: white;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-radius: 40px;
}
.input-container .input-wrap input{
  margin: 8px 10px 0 10px;
}